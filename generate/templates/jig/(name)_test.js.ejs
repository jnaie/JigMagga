steal('unit', function () {
    "use strict";

    module("<%= Namespace %>.Jig.<%= Name %>", {
        setup: function () {
            F.open("/<%= namespace %>/jig/<%= name %>/<%= name %>.html");
        }
    });

    test("visible Test", function () {
        //noinspection JSLint
        F(".<%= namespace %>-jig-<%= name %>").visible(function () {
            ok(true, "Section is visible");
        });
        F(".<%= namespace %>-jig-<%= name %> div").visible(function () {
            ok(true, "Section is not empty");
        });
    });
});